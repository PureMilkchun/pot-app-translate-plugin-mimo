{
  "id": "plugin.com.pot-app.mimo",
  "plugin_type": "translate",
  "icon": "mimo.svg",
  "display": "XiaoMi MiMo",
  "homepage": "https://github.com/pot-app/pot-app-translate-plugin-mimo",
  "needs": [
    {
      "key": "apiKey",
      "display": "API密钥",
      "type": "input"
    },
    {
      "key": "model",
      "display": "模型选择",
      "type": "select",
      "options": {
        "mimo-v2-flash": "mimo-v2-flash",
        "mimo-v2": "mimo-v2"
      },
      "default": "mimo-v2-flash"
    },
    {
      "key": "baseUrl",
      "display": "API地址(可选)",
      "type": "input",
      "default": "https://api.xiaomimimo.com/v1"
    }
  ],
  "language": {
    "auto": "auto",
    "zh_cn": "zh",
    "zh_tw": "zh_HANT",
    "en": "en",
    "ja": "ja",
    "ko": "ko",
    "fr": "fr",
    "es": "es",
    "ru": "ru",
    "de": "de",
    "it": "it",
    "tr": "tr",
    "pt_pt": "pt",
    "pt_br": "pt",
    "vi": "vi",
    "id": "id",
    "th": "th",
    "ms": "ms",
    "ar": "ar",
    "hi": "hi",
    "mn_cy": "mn",
    "mn_mo": "mn",
    "km": "km",
    "nb_no": "no",
    "nn_no": "no",
    "fa": "fa"
  }
}
